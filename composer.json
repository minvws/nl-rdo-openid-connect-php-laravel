{
    "name": "minvws/nl-rdo-openid-connect-php-laravel",
    "description": "OpenID Connect client for Laravel",
    "type": "library",
    "license": "EUPL-1.2",
    "autoload": {
        "psr-4": {
            "MinVWS\\OpenIDConnectLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MinVWS\\OpenIDConnectLaravel\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MinVWS\\OpenIDConnectLaravel\\OpenIDConnectServiceProvider"
            ]
        }
    },
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.6",
        "vimeo/psalm": "^5.8",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.7",
        "slevomat/coding-standard": "^8.8"
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit",
            "vendor/bin/phpstan analyse",
            "vendor/bin/psalm",
            "vendor/bin/phpcs"
        ],
        "phpcs": "vendor/bin/phpcs",
        "phpcbf": "vendor/bin/phpcbf",
        "phpstan": "vendor/bin/phpstan",
        "psalm": "vendor/bin/psalm"
    },
    "config": {
        "lock": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
